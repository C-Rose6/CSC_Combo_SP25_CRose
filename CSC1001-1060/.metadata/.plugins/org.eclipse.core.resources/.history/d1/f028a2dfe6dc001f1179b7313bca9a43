
public class Time {

	public static void main(String[] args) {
		
		int hour = 12;
		int minute = 39;
		System.out.println("The current time is, ");
		System.out.print(hour);
		System.out.print(":");
		System.out.print(minute);
		System.out.print(".");
		
		// TODO Auto-generated method stub

	}

}
